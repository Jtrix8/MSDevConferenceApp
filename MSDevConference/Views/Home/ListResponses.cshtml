@model System.Collections.Generic.IEnumerable<MSDevConference.Models.WebinarInvites>

@{
    ViewData["Title"] = "ListResponses";
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>List Responses</h2>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone);
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WillJoin)
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WillJoin)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {})
                @Html.ActionLink("Details", "Details", new {})
                @Html.ActionLink("Delete", "Delete", new {})
            </td>
        </tr>
    }
    </tbody>
</table>